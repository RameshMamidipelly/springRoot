<beans xmlns="http://www.springframework.org/schema/util"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:beans="http://www.springframework.org/schema/beans"
	xmlns:tool="http://www.springframework.org/schema/tool"
	targetNamespace="http://www.springframework.org/schema/util"
	elementFormDefault="qualified" attributeFormDefault="unqualified">
	<bean id="car" class="bean.Car">
		<constructor-arg index="1">
			<bean class="bean.Engine">
				<constructor-arg index="1">
					<util:map map-class="java.util.HashTable">
						<entry key="123" value="234" />
						<entry key="098" value="22342" />
					</util:map>
				</constructor-arg>
				<constructor-arg index="0">
					<util:set set-class="java.util.TreeSet">
						<value>SINLG</value>
						<value>DOUBLE</value>
					</util:set>
				</constructor-arg>
			</bean>
		</constructor-arg>
		<constructor-arg index="0">
			<util:list list-class="java.util.Vector">
				<value>ramesh</value>
				<value>rammi</value>
			</util:list>
		</constructor-arg>
	</bean>
</beans>